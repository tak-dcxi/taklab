/**
 * コンソールエラー Prop `className` did not match. の発生を防止します。
 * @see https://zenn.dev/nbr41to/articles/c0c691653e3d55
 */

{
  "presets": ["next/babel"],
  "plugins": [
    [
      "styled-components",
      {
        "ssr": true,
        "displayName": false,
        "preprocess": false
      }
    ]
  ]
}
